{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/send-notification",
      "dest": "app.js"
    }
  ],
  "crons": [
    {
      "path": "/send-notification",
      "schedule": "0 10 5,10,15,25,30 * *"
    }
  ]
}